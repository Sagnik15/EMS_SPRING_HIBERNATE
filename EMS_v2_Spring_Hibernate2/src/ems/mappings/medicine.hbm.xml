<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
    
      <hibernate-mapping>
    <class table="EMS_V2_medicine" name="ems.beans.MedicineBean">
    <id name="medId">
    <column name="medId"></column>
    <generator class="assigned"></generator>
    </id>
   
    <property name="medName">
    <column name="medName"></column>
    </property>
    
    <property name="medPrice">
    <column name="medPrice"></column>
    </property>
    
    </class>
    
    
    </hibernate-mapping>