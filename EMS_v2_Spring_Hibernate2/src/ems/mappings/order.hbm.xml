<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
    
      <hibernate-mapping>
      
      <class table="EMS_V2_order" name="ems.beans.OrderBean">
    <id name="order_Id">
    <column name="order_Id"></column>
    <generator class="assigned"></generator>
    </id>
   
    
    <property name="medId">
    <column name="medId"></column>
    </property>
    
    <property name="supplierId">
    <column name="supplierId"></column>
    </property>
    
     <property name="supplierName">
    <column name="supplierName"></column>
    </property>
    
    <property name="orderDate">
    <column name="orderDate"></column>
    </property>
    
   
    
    <property name="quantity">
    <column name="quantity"></column>
    </property>
    
    </class>
      
      
      </hibernate-mapping>